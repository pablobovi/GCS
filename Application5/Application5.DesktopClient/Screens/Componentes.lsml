  <Application.Methods>
    <ApplicationMethod
      Name="ShowComponentes">
      <ApplicationMethod.Attributes>
        <ShowScreenMethod
          TargetScreen="Componentes" />
      </ApplicationMethod.Attributes>
    </ApplicationMethod>
